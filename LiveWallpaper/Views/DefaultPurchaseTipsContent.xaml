<UserControl
    x:Class="LiveWallpaper.Views.DefaultPurchaseTipsContent"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:lan="clr-namespace:MultiLanguageForXAML;assembly=MultiLanguageForXAML"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <StackPanel>
        <TextBlock HorizontalAlignment="Center" FontSize="14">
            <Run lan:Xaml.Key="donate_title" />
        </TextBlock>
        <Image
            Height="200"
            Margin="10"
            HorizontalAlignment="Center"
            Source="/LiveWallpaper;component/Res/Imgs/donate.jpg" />
    </StackPanel>
</UserControl>
